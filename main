// Getting input via STDIN
const readline = require("readline");

const inp = readline.createInterface({
  input: process.stdin
});

const userInput = [];

inp.on("line", (data) => {
  userInput.push(data);
});

inp.on("close", () => {
  //start-here
  //Your code goes here â€¦ replace the below line with your code logic 
  
  let a = Math.round(userInput)
  var result
  if (a%2 === 0){
   console.log ("Even")
  }
  else if(a%2 !==0){
      console.log("Odd")
  }
  else{
      console.log ("Zero")
  }
});
